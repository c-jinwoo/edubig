	<script>
		$(document).scroll(function(){
			setTimeout(function(){
				$(".cont_apply").css("bottom","0");	
			},100);	
		});
		
		function frmSubmit(cno){
			$("#courseno").val(cno);
			$("#frm").submit();
		}		
	</script>
