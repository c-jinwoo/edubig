@import url(./font.css);

body {
  width: 100%;
  height: 100%;
  background: url(../img/loading.gif) no-repeat;
  background-size: 100% 100%;
  overflow:hidden;
  margin:0px;
  padding:0px;
}

.content {
  position: absolute;
  left: 0px;
  top: 0px;
  width: 1012px;
  height: 630px;
}

.content > .video1 {
  position: absolute;
  width: 100%;
  height: auto;
  z-index: 100;
}

.content > .controlBar {
  position: absolute;
  bottom : 0px;
  z-index: 200;
/*   background: hsla(0, 10%, 0%, 0.6); */
  background: #001219;
/*   padding-bottom: 40px; */
  padding-top: 0px;
  width: 1012px;
  right:0px;
  transition-duration: 0.4s;
/*   border-top-left-radius: 20px; */



}


.content > .controlBar .button> button:hover,
.content > .controlBar .button> button:focus{
    background-position: -40px 0px;
}

.content > .controlBar > .button> button.toggle{
    background-position: -0px -40px;
}

.content > .controlBar > .button > button.toggle:hover,
.content > .controlBar > .button > button.toggle:focus{
    background-position: -40px -40px;
}




