	    }

		// 결제 정보 설정
		function fnSetUpInfo(){
			
			var payType = $('input[name=PayM]:checked').val();
			
			if(!$('input[name=PayM]:checked').is(":checked")){
				fnModalMsg("결제 구분을 선택해주세요.");
			}
			
			$('input[name=LGD_CUSTOM_USABLEPAY]').val('SC' + payType.substr(2,4));
			window.open("about:blank","payWinPop","width=650,height=650");
			var frm = document.frm;
			frm.action = "/ccedu/payTest/openUrl.do";
			frm.target = "payWinPop";
			frm.submit();
		}
		
		function fnSubmit(){
			$("#address2").val();
			var frm = document.frm;
			frm.target = "";
			frm.action = "/ccedu/payTest/ccLicenseCrud.do";			
			frm.submit();
		}
	
	</script>
</head>


