				} else if($('#email3 option:selected').val() == '') {
					$('#email2').val('');
				}
			})
		});
	
	
		//우편번호 콜백 스크립트
		function fnZipSelect(zip_code, adres){
			
			var zipCode = Number(zip_code);
			
			zipCode += "";
			
			if(zipCode.length == 4){
				zipCode  = "0"+zipCode;
			}
			
			 $('#addrCode').val(zipCode);
		     $('#address').val(adres);
		     $("#popPost").find("#close").click();
		} 
			
