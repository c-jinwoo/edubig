<script type="text/javascript">

$(function(){ 

	var retcode = '${retcode}';
	var joinYn = '${joinYn}';
	if(retcode == 'B000'){
		
		fnConfirm("실명인증이 완료되었습니다.", function(result) {
			
			if(joinYn != '' && joinYn == 'Y'){
				opener.document.frm.confirmGubun.value = 'IPIN';
				var ssn2 = '${result[11]}';
				var sex  = '${result[9]}';
				opener.document.frm.ssnCd.value= ssn2.substring(0,1);
				if(ssn2 != null && ssn2.length > 1){
					if(sex == '1'){
						$('#sex').val('M');
						opener.document.frm.sex.value= 'M';
					}else{
						$('#sex').val('F');
						opener.document.frm.sex.value= 'F';
					}
				}
				opener.document.frm.brthdy.value = ssn2.substr(2,7);
				opener.document.frm.userName.value = '${result[6]}';
				opener.document.frm.di.value = '${result[0]}';
				opener.document.frm.submit();
				
			}else{
				
				opener.document.frm.confirmGubun.value = 'IPIN';
				opener.document.frm.di.value = '${result[0]}';
				opener.document.getElementById('certYn').innerHTML ='인증';
				
			}
			self.close();
			return false;
		});
		return false;
		
		
	}else{
		
		fnConfirm("실명인증을 실패하였습니다.", function(result) {
			if(joinYn != '' && joinYn == 'Y'){
				opener.document.frm.action = '/ccedu/user/memberJoinStep02.do';
			}else{
				opener.document.frm.action = '/ccedu/common/oldmemCert.do';
			}
			opener.document.frm.submit();
			self.close();
			return false;
		});
		return false;
		
	}
	
});



	
