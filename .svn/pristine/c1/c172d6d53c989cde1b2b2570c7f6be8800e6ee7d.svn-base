<head>
	<title>접수조회/수정 > 원서접수 > 신용상담사 > 신용회복위원회 신용교육원</title>
	<%@ include file="/WEB-INF/jsp/ccedu/inc/head_inc.jsp"%>
	<script type="text/javascript">
	
		$(document).ready(function() {
			
			if('${examCheckMap}' == null || '${examCheckMap}' == '' ) {
				fnModalMsg("접수한 내용이 없습니다.");
			}
		});
	
	
		// 상세보기
		function detailInfo(ccNo, type, ccAppNo) {
			$("#pageMode").val("inquiry")
			$("#ccAppNo").val(ccAppNo);
			$("#ccNo").val(ccNo);
			$("#mtAppType").val(type);
			$("#frm").attr("action", "/ccedu/cc/applicationCrud.do");
			$("#frm").submit();
		}
		
		// 접수증 출력
		function printReceipt(ccNo, type, ccAppNo,today,examdt) {
			
			
			if(today<examdt){
				alert(examdt);
			}
			
			
			$("#ccAppNo").val(ccAppNo);
			$("#ccNo").val(ccNo);
			$("#mtAppType").val(type);
			
			var w1 =1070;
			var h1 = 900;
			var url = "/ccedu/cc/printReceipt.do";
			
			window.open( url+"?"+$("#frm").serialize()
				,"receipt","width="+w1+" height="+h1+", top=0, left=0, resizable=yes, status=no, menubar=no, toolbar=no, scrollbars=yes, location=no");
		}
		
	</script>
