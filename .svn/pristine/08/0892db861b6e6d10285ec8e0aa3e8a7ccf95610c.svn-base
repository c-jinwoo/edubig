	.all_menu {width:140px; height:calc(100vh - 50px); padding-top:20px;}
	.all_menu > li > a {padding:12px 0 12px 13px; font-size:15px; line-height:16px;}
	.all_menu > li.active > a:before {right:15px; width:12px; height:12px; margin-top:-7px; background-size:12px 12px;}
	.all_menu > li.active .in_depth {padding-left:140px;}
	.all_menu > li > .in_depth .in_depth_tit_p {padding:20px 0 10px 15px; font-size:20px; line-height:41px;}
	.all_menu > li > .in_depth .in_depth_tit_p:before {left:15px; width:33px; height:2px;}
	.all_menu > li > .in_depth .in_depth_menu {margin-top:5px;}
	.all_menu > li > .in_depth .in_depth_menu > li > a {padding-left:15px; font-size:14px; line-height:40px;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.inul:before {right:15px; width:10px; height:2px; margin-top:-1px;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.inul:after {right:19px; width:2px; height:10px; margin-top:-5px;}
	.all_menu > li > .in_depth .in_depth_menu > li > ul > li > a {padding-left:30px; font-size:13px; line-height:33px;}

	#location_wrap {height:36px;}
	.location .btn_home {display:none;}
	.location_nav {display:table; width:100%;}
	.location_nav > li {display:table-cell; float:none; min-width:auto; font-size:11px; line-height:36px;}
	.location_nav > li > span {display:block; padding:0 12px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
	.location_nav > li.location_toggle {border-right:none;}
	.location_nav > li > a {padding:0 12px; font-size:11px; line-height:36px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
	.location_nav > li.location_toggle > a {padding-right:22px;}
	.location_nav > li.location_toggle > a:before {right:8px; width:9px; height:6px; margin-top:-3px; background-size:8px 5px;}
	.location_nav > li.location_toggle ul li a {padding:0 12px; font-size:11px; line-height:36px;}

	#content {min-height:calc(100vh - 317px); padding:20px 0 60px;}

	.footlinks_fmlst .footlinks {padding:6px;}
	.footlinks_fmlst .footlinks li a {font-size:11px; line-height:30px;}
	.footlinks_fmlst .fmlst {border-bottom-width:1px;}
	.footlinks_fmlst .fmlst > a {padding:0 15px; font-size:11px; line-height:38px;}
	.footlinks_fmlst .fmlst > a:before {right:15px; width:15px; height:9px; margin-top:-5px; background-size:15px 9px;}
	.footlinks_fmlst .fmlst .site_list {bottom:38px; padding-top:5px;}
	.footlinks_fmlst .fmlst .site_list li a {padding-left:15px; font-size:11px; line-height:32px;}
	.footlinks_fmlst .fmlst .site_list li a span {line-height:16px;}

	.foot_info {padding:15px; font-size:10px; line-height:16px; letter-spacing:-1px;}
	.foot_info .copyright {margin-top:10px;}
}
