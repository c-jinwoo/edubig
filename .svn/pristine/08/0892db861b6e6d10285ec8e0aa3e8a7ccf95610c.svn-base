.util_section a:first-child:before {display:none;}

.util_section .top_logo {width:146px; height:100%; display:inline-block;}

.util_section .top_r {position:absolute; right:0; top:18px;}
.util_section .top_r .util_sec_r{margin-top: 3px;}
.util_section .top_r .util_sec_r li {float:left; padding:0 7px;}
.util_section .top_r .util_cs a{font-weight:bold;}

.util_section .top_r .util_sec_r .util_cs {position:relative; width:65px; height:20px;}
.util_section .top_r .util_sec_r .util_cs:hover{background-position-y:-30px;}
.util_section .top_r .util_sec_r .util_cs:hover > a{color:#f13748;}
.util_section .top_r .util_sec_r .util_cs:hover .util_ly{display:block;}

.util_section .top_r .util_ly{position:absolute; top:20px; left:-35px; background:#fff; border:1px solid #222; z-index:1220; display:none; padding:10px 0 10px; width:130px;}
.util_section .top_r .util_ly li{width:100%; padding-left:25px;}
.util_section .top_r .util_ly li a{font-size:12px; color:#121212; line-height:24px; font-weight:normal;}
.util_section .top_r .util_ly li a:hover{border-bottom:1px solid #6c6c6c; font-weight:600;}

.util_section .top_r .util_mylect a{font-weight:bold;}
.util_section .top_r .util_sec_r .util_mylect {position:relative; width:65px; height:20px;}
.util_section .top_r .util_sec_r .util_mylect:hover{background-position-y:-30px;}
.util_section .top_r .util_sec_r .util_mylect:hover > a{color:#f13748;}
.util_section .top_r .util_sec_r .util_mylect:hover .util_ly{display:block;}

.util_section .top_r .util_sec_r .util_mylect {position:relative; width:65px; height:20px;}
.util_section .top_r .util_sec_r .util_mylect:hover{background-position-y:-30px;}
.util_section .top_r .util_sec_r .util_mylect:hover > a{color:#f13748;}
.util_section .top_r .util_sec_r .util_mylect:hover .util_ly{display:block;}

.util_section .top_r .util_ly{position:absolute; top:20px; left:-35px; background:#fff; border:1px solid #222; z-index:1220; display:none; padding:10px 0 10px; width:130px;}
.util_section .top_r .util_ly li{width:100%; padding-left:25px;}
.util_section .top_r .util_ly li a{font-size:12px; color:#121212; line-height:24px; font-weight:normal;}
.util_section .top_r .util_ly li a:hover{border-bottom:1px solid #6c6c6c; font-weight:600;}

/* 우측 퀵메뉴(상하 이동하는배너) */
.quick_menu_renew{position:absolute; top:190px; right:0; width:118px; z-index:9999;}
.quick_menu_renew .quick_menu_tit{display:block;height:36px;font-size:0;text-indent: -9999;background:url('../images/quick_menu.png') no-repeat center center;margin: 0 auto;}
.quick_menu_renew ul{margin-bottom:13px;}
.quick_menu_renew ul li{width:100%;background:#fff;}
.quick_menu_renew ul li a{display:block;padding:12px 0;border:1px solid #d7d7d7;border-top:0;font-size:12px;text-align:center;color:#797979;}
.quick_menu_renew ul li:first-child a{color:#cc0000;}
.quick_menu_renew > a{font-size:0;text-indent: -9999;display:block;height:23px;background:url('../images/quick_top.png') no-repeat center center;}
.quick_menu_renew > a.top{height:22px;/* background-position:-350px -326px; */}






.quick_menu_renew .btn_quick a{position:absolute; top:276px; left:-23px; width:22px; height:69px; font-size:0; line-height:0; z-index:10;}
.quick_menu_renew .btn_quick .open{display:none}
.quick_menu_renew .btn_quick .close{background-position:-39px -33px}
.quick_menu_renew .btn_quick .open{display:block}
.quick_menu_renew .btn_quick .close{display:none; background-position:-39px -33px}
.quick_menu_renew .btn_quick a{top:65px; left:-42px; width:42px; height:150px; background:#f4f4f4; border:1px solid #cdcdcd; border-right:0; box-sizing: border-box; padding-top:60px; text-align:center;}
.quick_menu_renew .btn_quick a span{display:inline-block; width:14px; font-size:14px; color:#222; line-height:16px;}
.quick_menu_renew .btn_quick a:before{content:''; display:block; width:12px; height:12px; position:absolute; top:25px; right:11px; border:1px solid #222; border-width:0 0 1px 1px;transform:rotate(45deg);-webkit-transform:rotate(45deg);-ms-transition: all 0.3s ease-out;-webkit-transition: all 0.3s ease-out;transition: all 0.3s ease-out;}
.quick_menu_renew .btn_quick a.close:before{border-width:1px 1px 0 0; right:18px}
.quick_menu_renew .btn_quick.open a:before{-webkit-transform:rotate(-135deg); transform:rotate(-135deg); right:18px}


.gnb_section {position:relative; width:100%; height:50px; border-top:1px solid #cdcdcd; border-bottom:1px solid #cdcdcd; background:#FFF;z-index:999999;top:0;}
.gnb_section .gnb_outer {height:50px; position:relative;}
.gnb_section .gnb_box {width:1080px; margin: 0 auto;}
.gnb_items {position:relative; width:100%; font-size:0;}
.gnb_items > ul > li {display:inline-block; vertical-align:top; width:calc(100% / 6); text-align:center;}
.gnb_items > ul > li > a {height:50px; padding:0 36px; display:block; font-size:16px; line-height:51px; font-weight:600; color:#666;}

/*#gnb > li:last-child > a {padding-right:0;}*/
#gnb > li.active > a,
#gnb > li > a:hover,
#gnb > li > a:focus,
#gnb > li > a:active{color:#e71a3f;}

.gnb_s_layer{display:none; width:100%; height:auto; position:absolute; left:0; top:49px; background-color:#eaeaf0; z-index:1200;}
.gnb_s_layer ul{text-align:left; font-size:0;}
.gnb_s_layer li{display:inline-block; vertical-align:middle;}
.gnb_s_layer a{display:block; padding:0 24px; font-size:16px; line-height:65px; color:#666;}
.gnb_s_layer a:hover{color:#e71a3f;}
.gnb_s_layer .mycla{text-align: right;}
.on.gnb_course_layer{display:block;}
.on.gnb_myclass_layer{display:block;}

/*2018-08-28 김동인 작업 S*/
#gnb > li > .in_depth {position:absolute; left:0; width:1px; height:1px; padding:0; border:1px solid #ccc; background:#f9f9f9; text-indent:-9999px; overflow:hidden;}
#gnb > li.active > .in_depth {width:100%; height:auto; padding:0; text-indent:0;}
#gnb > li > .in_depth .in_depth_menu > li > a {display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center;  float:left; width:20%; height:49px; padding:0 0 0 30px; color:#4e5583; font-size:15px; line-height:19px; font-weight:bold; word-break:keep-all; background-color:#e5e5e5}
#gnb > li > .in_depth .in_depth_menu > li {border-top:1px solid #d5d5d5; overflow:hidden;}
/*2018-08-28 김동인 작업 E*/

#gnb > li > .in_depth .in_depth_tit_p {position:absolute; top:0; width:330px; height:100%; padding:0 30px;}
#gnb > li > .in_depth .in_depth_tit_p .tit {position:absolute; top:50%; margin-top:-61px; padding-bottom:15px; font-size:22px; line-height:32px;}

#gnb > li > .in_depth .in_depth_tit_p .tit_p {position:absolute; top:50%; margin-top:0; font-size:13px; line-height:23px; opacity:0.7;}
#gnb > li > .in_depth .in_depth_menu {padding-left:330px; letter-spacing:-1px;}

#gnb > li > .in_depth .in_depth_menu > li:first-child {border-top:none;}

#gnb > li > .in_depth .in_depth_menu > li > ul {float:left; width:80%;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li {float:left; width:25%;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a {display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center; position:relative; width:100%; height:49px; padding:0 0 0 35%; white-space:nowrap; word-break:keep-all;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:hover,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:focus,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:active {color:#4458b7; font-weight:bold;}
/*2018-08-28 김동인 작업 S*/
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a > span {display:inline-block; font-size:13px; line-height:21px;}
/*2018-08-28 김동인 작업 E*/
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:hover span,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:focus span,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:active span {border-bottom:2px solid #4458b7;}

.mobile_head {display:none;}

#location_wrap {height:40px; background:#255880;}
.location .btn_home {float:left; width:50px; height:40px; padding-top:14px; background:rgba(255,255,255,0.85); text-align:center;}
.location_nav > li {float:left; min-width:180px; border-right:1px solid rgba(255,255,255,0.5); color:#fff; font-size:13px; line-height:40px;}
.location_nav > li > span {padding-left:20px;}
.location_nav > li > a {display:block; position:relative; padding:0 20px; color:#fff; font-size:13px; line-height:40px; z-index:10;}
.location_nav > li.location_toggle > a {padding-right:40px; font-weight:bold;}
.location_nav > li.location_toggle > a:before {content:''; position:absolute; top:50%; right:16px; width:14px; height:8px; margin-top:-4px; background:url('../images/location_updown.png') no-repeat center center;}
.location_nav > li.location_toggle.on > a:before {transform:rotate(180deg); -webkit-transform:rotate(180deg);}
.location_nav > li.location_toggle {position:relative;}
.location_nav > li.location_toggle ul {display:none; position:absolute; left:0; width:100%; background:#255880; z-index:9;}
.location_nav > li.location_toggle.on ul {display:block;}
.location_nav > li.location_toggle ul li {border-top:1px solid rgba(255,255,255,0.5);}
.location_nav > li.location_toggle ul li a {display:block; padding-left:20px; color:#fff; font-size:13px; line-height:40px;}
.location_nav > li.location_toggle ul li a:hover {background:rgba(255,255,255,0.2);}

#lnb {float:left; width:250px; padding:50px 0; text-align:left;}
#lnb h2 {border:1px solid gray; background:url('../images/bg_lnb_head.png') no-repeat right bottom #ffffff; color:#000; font-size:22px; line-height:90px; text-align:center;}
.lnb_menu > li {position:relative; border:1px solid #a8a8a8; border-top:none;}
.lnb_menu > li.on:before {content:''; position:absolute; top:0; left:-1px; width:2px; height:100%; background:#255880; z-index:2;}
.lnb_menu > li > a {display:block; position:relative; padding:15px 35px 15px 25px; background:#ffffff; font-size:15px; line-height:22px; font-weight:bold;}
.lnb_menu > li.on > a {color:#000;}
.lnb_menu > li.iscont > a:before {content:''; position:absolute; top:26px; right:20px; width:10px; height:2px; background:#444;}
.lnb_menu > li.iscont > a:after {content:''; position:absolute; top:22px; right:24px; width:2px; height:10px; background:#444;}
.lnb_menu > li.on > a:before {background:#f08200;}
.lnb_menu > li.on > a:after {display:none;}
.lnb_menu > li > ul {display:none; padding:10px 0; background:#fafafa; overflow:hidden;}
.lnb_menu > li.on > ul {display:block;}
.lnb_menu > li > ul > li > a {display:block; padding:5px 0 5px 34px; font-size:13px; line-height:15px;}
.lnb_menu > li > ul > li > a:hover,
.lnb_menu > li > ul > li > a.on {color:#f08200; font-weight:bold;}

#content {min-height:calc(100vh - 313px);text-align:left;}

#footer {clear:both; position:relative; z-index:90;}
#footer .con_center {position:relative;}
.footlinks_fmlst {background:#222; letter-spacing:-1px;}
.footlinks_fmlst .footlinks {width:calc(100% - 150px); overflow:hidden;}
.footlinks_fmlst .footlinks li {width:calc(100% / 7); float:left;}
.footlinks_fmlst .footlinks li a {display:block; position:relative; color:#dcdcdc; font-size:13px; line-height:40px; text-align:center;}
.footlinks_fmlst .footlinks li a:before {content:''; position:absolute; top:50%; left:0; width:1px; height:15px; margin-top:-7px; background:rgba(255,255,255,0.2);}
.footlinks_fmlst .footlinks li a .ico_mouse {display:inline-block; padding-right:13px; background:url('../images/ico_foot_mouse.png') no-repeat right center;}

.footlinks_fmlst .fmlst {position:absolute; top:8px; right:0;}
.footlinks_fmlst .fmlst > a {display:block; position:relative; width:140px; height:25px; padding-left:15px; background:#fff; color:#444; font-size:13px; line-height:25px; font-weight:bold; z-index:50; letter-spacing:0px;}
.footlinks_fmlst .fmlst > a:before {content:''; position:absolute; top:50%; right:15px; width:11px; height:7px; margin-top:-4px; background:url('../images/ico_foot_familysite.png') no-repeat 0 0;}
.footlinks_fmlst .fmlst.open > a:before {transform:rotate(180deg);}
.footlinks_fmlst .fmlst .site_list {position:absolute; width:100%; bottom:25px; left:-9999px; padding-top:10px; background:#fff; overflow:hidden; z-index:51;}
.footlinks_fmlst .fmlst.open .site_list {left:0;}
.footlinks_fmlst .fmlst .site_list li a {display:block; padding-left:10px; color:#333; font-size:13px; line-height:35px;}
.footlinks_fmlst .fmlst .site_list li a span {display:inline-block; line-height:18px;}
.footlinks_fmlst .fmlst .site_list li a:hover span,
.footlinks_fmlst .fmlst .site_list li:hover a{color:#fff; font-family:ngb;background:#f13748;}

.foot_info {position:relative; width:100%; background:#e3e3e3; color:rgba(255,255,255,0.7); font-size:12px; line-height:22px; z-index:50;}
