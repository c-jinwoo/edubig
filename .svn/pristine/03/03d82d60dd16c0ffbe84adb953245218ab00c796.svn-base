@charset "utf-8";

/* 폰트 */
@font-face {
  font-family: 'nanumsquare';
  font-style: normal;
  font-weight: normal;
  src: url('font/NanumSquareR.eot');
  src: local('Nanum Square Regular'),
       local('NanumSquareR'),
       url('font/NanumSquareR.eot?#iefix') format('embedded-opentype'),
       url('font/NanumSquareR.woff2') format('woff2'),
       url('font/NanumSquareR.woff') format('woff'),
       url('font/NanumSquareR.ttf') format('truetype');
}
@font-face {
  font-family: 'nanumsquare';
  font-style: normal;
  font-weight: bold;
  src: url('font/NanumSquareB.eot');
  src: local('Nanum Square Bold'),
       local('NanumSquareB'),
       url('font/NanumSquareB.eot?#iefix') format('embedded-opentype'),
       url('font/NanumSquareB.woff2') format('woff2'),
       url('font/NanumSquareB.woff') format('woff'),
       url('font/NanumSquareB.ttf') format('truetype');
}

/* 좌측 배너 */
.main_pop{position: absolute;z-index: 9999;background: #fff;top: 0;bottom: 0;left: 0;right: 0;width: 300px;height: 300px;}
.main_pop .swiper-slide .pc {position:absolute; top:0; left:50%; width:3000px; height:100%; margin-left:-1500px; text-align:center;}
.main_pop .swiper-slide .pc a {display:block;}
.main_pop .swiper-slide .pc img {position:relative;}
.main_pop .swiper-slide .mobile {display:none;}
.main_pop .swiper_prev_next .swiper-button-prev {top:50%; left:5px; width:28px; height:42px; padding:5px; margin:-21px 0 0 0; border:none; background:url('../images/btn_main_pop_prev.png') no-repeat center center; overflow:hidden; text-indent:-9999px; z-index:15;}
.main_pop .swiper_prev_next .swiper-button-prev.swiper-button-disabled {opacity:.6; cursor:pointer; pointer-events:auto ;}
.main_pop .swiper_prev_next .swiper-button-next {top:50%; right:5px; width:28px; height:42px; padding:5px; margin:-21px 0 0 0; border:none; background:url('../images/btn_main_pop_next.png') no-repeat center center; overflow:hidden; text-indent:-9999px; z-index:15;}
.main_pop .swiper_prev_next .swiper-button-next.swiper-button-disabled {opacity:.6; cursor:pointer; pointer-events:auto ;}
.main_pop .ctl_btn .cb_none {display:inline-block; position:absolute; right:140px; bottom:10px; padding:0; margin:0; border:none; border-bottom:1px solid #3e3e3e; background:none; color:#333; font-size:13px; line-height:20px; letter-spacing:-1px; z-index:20;}
.main_pop .ctl_btn .btn_close {position:absolute; right:140px; top:10px; width:16px; height:16px; padding:0; margin:0; border:none; background:url('../images/btn_main_close_pop.png') no-repeat center center; text-indent:-9999px; overflow:hidden; z-index:25;}

/* 본문 */
#m_content {width: 100%;}

/* 상단 : 슬라이더 */
.slide_nav_wrapper{position:absolute;top:0;right:50%;width:200px;height:100%;margin-right:-540px;background:#000;z-index:1;opacity: 0.8;}
.slide_nav{position:relative;width:100%;height:100%;}
.slide_nav_wrapper a{position:relative;display:block;width:100%;height:58px;text-align:center;line-height:58px;font-size:15px;background:#000;color:#FFF;transition:0.75s;border-bottom:1px solid #666;cursor:pointer;}
.slide_nav_wrapper a.active{background:#FFF;color:#000;}

.main_top_slides{position:relative; width:100%; height:410px;}
.main_top_slides .slide{position:relative; width:100%; height:auto; overflow:hidden;} 
.main_top_slides .slide .slide_wrap{position:relative; width:100%;}

/* 중간 공통 */
.main_div{position: relative;width:100%;height:auto;padding:100px 0;}
.main_div_title{text-align:center;}
.main_div_title h3 {font-family:'ngb'; font-size:44px; letter-spacing:-2px;}
.main_div_title p {margin-top:15px; font-size:15px; letter-spacing:-1px;}
.main_div_body {position:relative;margin-top:75px;}

/* 중간1 : 강사 */
.main_div_teacher{background-color:#f3f6f9;}
.main_div_teacher .main_div_title h3{color:#464646;}
.main_div_teacher .main_div_title p{color:#666;}
.main_star_list{position:relative; width:100%; height:400px; margin:0px auto; overflow:hidden;}
.main_star_list ul{text-align:center;}
.main_star_list li{position:relative; width:25%; display:inline-block; vertical-align:top; border:1px solid #c8c8c8;}
.main_star_list li + li:after{position:absolute; width:1px; height:100%; left:0; top:0; content:''; background-color:rgba(243,246,249,0.5);}
.main_star_list .wrap{position:relative; display:block;}
.main_star_list .img_box{position:relative; width:100%; height:400px; overflow:hidden; background-color:#fff;}
.main_star_list .img_box img{position:relative; width:200%; left:-50%; top: -15px;}
.main_star_list .cont_box{position:absolute; left:0; bottom:0; width:100%; padding:17px 0; text-align:center; color:#fff; background-color:rgba(0,0,0,0.5);}
.main_star_list .cont_box span{font-size:15px; opacity:0.55;}
.main_star_list .cont_box strong{margin-left:15px; font-family:'ngb'; font-size:18px;}
.main_star_list .cont_box a{display:none;}
.main_star_list .wrap:hover .img_box img{transform:scale(1.05);}
.main_star_list .wrap:hover{z-index:1;}
.main_star_list .wrap:hover .cont_box{height:100%;}
.main_star_list .wrap:hover .cont_box .in_box{position:absolute; width:100%; left:0; bottom:40px;}
.main_star_list .wrap:hover .cont_box a{display:inline-block;}
.main_star_list .wrap:hover .cont_box strong{position:absolute; left:0; top:-35px; right:0; margin:0; text-align:center; font-size:27px; }
.main_star_list .wrap:hover .cont_box span{margin:0 0 20px; display:block; font-size:16px; opacity:1;}
.main_star_list .img_box,
.main_star_list .img_box img,
.main_star_list .cont_box{-ms-transition:all 0.3s ease-out; -webkit-transition:all 0.3s ease-out; transition:all 0.3s ease-out;}
.main_star_list .btn_type_round {width: 160px; display: inline-block; text-align: center; font-family: 'ngb'; font-size: 16px; line-height: 46px; color: #fff; border: 2px solid #fff; border-radius: 50px; -ms-transition: all 0.3s ease-out; -webkit-transition: all 0.3s ease-out; transition: all 0.3s ease-out;}
.main_star_list .btn_type_round:hover{background-color:#fff;color:#333;}

/* 중간2 : 교재 */
.main_div_book{background-color:#232323;}
.main_div_book .main_div_title h3{color:#FFF;}
.main_div_book .main_div_title p{color:#666;}
.main_book_slide{text-align:center;}
.main_book_slide:focus{outline:none;}
.main_book_slide .main_book_img img{width:80%;height:auto; margin:0 auto;}
.main_book_slide .main_book_slide_title{font-size: 17px;color: #FFF;}
.main_book_slide .main_book_slide_vprice{font-size: 14px;text-decoration:line-through;color: #666;}
.main_book_slide .main_book_slide_rprice{font-size: 14px;font-weight: 700;color: #e2450e;}

/* 중간3 : 무료강의 */
.main_div_free{background-color:#FFF;}
.main_div_free .main_div_title h3{color:#232323;}
.main_div_free .main_div_title p{color:#666;}
.main_free_slide{text-align:center;}
.main_free_slide:focus{outline:none;}
.main_free_nav{padding-top:35px;}
.main_div_free .main_free_nav video{width:80%; height:auto; margin:0 auto;}

/* 하단 : 게시판요약 */
.main_customer{position:relative; width:100%; height:340px; background-color:#EDEDED; padding:50px 0;}
.main_customer .main_customer_inner{padding: 20px 0;}  
 
.info_box{position:relative;height: 240px; padding: 20px; background: #fff;float: left;}
.info_box:nth-child(1){width:31%;margin-right: 1.5%;}
.info_box:nth-child(2){width:31%;margin-right: 1.5%;}
.info_box:nth-child(3){width:35%;}

.info_title_wrapper{position:relative;}
.info_title{font-size:20px;font-weight: bold;color: #333;}

.info_event_wrapper{height:calc(100% - 40px);overflow:hidden;}
.info_event_tit{font-size: 18px;font-weight: bold;color: #000;margin-top: 20px;}
.info_event_cont{font-size: 15px;color: #666;margin-top: 20px;letter-spacing: -2px;}

.info_bbs .info_title_wrapper{border-bottom: 1px solid #dfdfdf;padding: 5px 0;}
.info_bbs ul {position:relative; top:20px; left:0; width:100%; height:auto; text-indent:0;}
.info_bbs li {padding:5px 0;}
.info_bbs li > a {display:block; position:relative; padding:0 85px 0 8px;}
.info_bbs li > a .tit {display:block; width:100%; color:#444; font-size:13px; line-height:20px; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.info_bbs li > a .date_area {display:inline-block; position:absolute; top:50%; right:8px; margin-top:-10px; color:#666; font-size:12px; line-height:20px;}
.info_bbs .btn_more {position: absolute;display: inline-block;top: 0;right: 0;padding: 10px;text-align: center;background: url(../images/ico_more_plus.png) no-repeat;background-position: 50% 50%;background-size: 8px 8px;background-color:#000;width: 8px;height: 8px;}

.info_call ul{margin:24px 0;}
.info_call ul li{color:#666666; line-height:34px; font-family:'nanumsquare';}
.info_call ul li em{display:inline-block; width:100px; font-size:17px; font-style: normal; font-weight: normal;}
.info_call ul li strong{font-weight:900; font-size:25px;}
.info_call ul li strong span{color:#2b768c;}
.info_call .info_call_time{line-height:20px; color:#666666;}
.info_call .info_call_time span{display:inline-block; width:60px; font-weight:bold; color:#333;}
.info_call .ico_call {width:72px; height:72px; position:absolute; right: 20px; top: 45%; transform:translateY(-50%); background:url('../images/ico_main_call.png') no-repeat center; background-size:72px;}

