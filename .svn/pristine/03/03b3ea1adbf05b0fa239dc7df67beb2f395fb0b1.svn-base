	height:150px;
	padding:15px;
	text-align:center;
}
.book_table td:nth-child(2){
	text-align:left;
}
.book_table td img{
	width:100%;
	height:100%;
}
.book_table td .book_title{
	font-weight:bold;
	font-size:16px;	
}
.book_table td .book_sub{
	font-size:12px;	
}
.book_table td .price_b{
	font-size:14px;
	text-decoration:line-through;
}
.book_table td .price_a{
	font-size:15px;
	font-weight:700;
	color:#e2450e;
}


