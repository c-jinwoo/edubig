.content > .controlBar .button> button:hover{
    background-position: -26px 0px;
}
.content > .controlBar > .button> button.toggle{
    background-position: -0px -26px;
}
.content > .controlBar > .button > button.toggle:hover{
    background-position: -26px -26px;
}
.content > .controlBar .button.movePrev> button:hover,
.content > .controlBar .button.moveNext> button:hover{
    background-position: -30px 0px;
}
.content > .controlBar > .button.movePrev> button.toggle,
.content > .controlBar > .button.moveNext> button.toggle{
    background-position: -0px -30px;
}
.content > .controlBar > .button.movePrev> button.toggle:hover,
.content > .controlBar > .button.moveNext > button.toggle:hover{
    background-position: -30px -30px;
}
