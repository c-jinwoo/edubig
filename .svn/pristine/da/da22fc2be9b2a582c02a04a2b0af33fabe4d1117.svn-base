.content > .controlBar > .nextAlert {
	position: absolute;
	width: 145px;
	height: 32px;
	right: 35px;
	bottom: 38px;
	transition-duration: 1.0s;
	background: url(../img/moveAlert.png) no-repeat;
	display: none;
}
.content > .controlBar > .timeLine {
	position: absolute;
	background: #444444;
	width: 30%;
	padding-top: 9px;
	top: 15px;
	z-index: 250;
	right: 600px;
}
.content > .controlBar > .timeLine > .progress {
	background: #35b7e8;
	width: 0%;
	position: absolute;
	padding-top: 9px;
	top: -0px;
}
.content > .controlBar > .soundLine {
	position: absolute;
	width: 45px;
	background: #444444;
	padding-top : 9px;
	right : 285px;
	top: 15px;
}
.content > .controlBar > .soundLine > .soundProgress {
  position: absolute;
  width: 50%;
  padding-top: 9px;
  top: -0px;
  background: #35b7e8;
}
.content > .printBtn {
