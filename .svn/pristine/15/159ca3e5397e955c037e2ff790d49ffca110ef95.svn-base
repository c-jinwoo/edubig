<%-- 										<c:if test="${rowData.PASS_YN eq 'Y'}"> 국가공인 신용상담사 자격시험에 합격하셨습니다. 축하드립니다.</c:if> --%>
<%-- 										<c:if test="${rowData.PASS_YN ne 'Y'}"> 금번 시험에 불합격하셨습니다. 다음 기회를 이용하시기 바라며, 신용상담사 자격시험에 관심을 가져주셔서 진심으로 감사드립니다.</c:if> --%>
<!-- 									</td> -->
<!-- 								</tr> -->
<!-- 							</table> -->
<!-- 						</div> -->
<!-- 					</div> -->

					<div class="c_btn_right_m_center">
<%-- 						<c:if test="${rowData.PASS_YN eq 'Y'}"> --%>
<!-- 							<a href="page" onclick="addInfo(); return false;" class="btn_blueline03">추가정보입력</a>&nbsp;&nbsp;&nbsp;&nbsp; -->
<%-- 						</c:if> --%>
						<a href="page" onclick="examPassYnListGo(); return false;" class="btn_blueline03">목록</a>
					</div>
					
					<!--// end -->
				</div>
				<!--// 본문내용 -->
				
			<!--  -->
			</div>
			<!--// content 본문 -->
			</form>
		<!-- con_cneter end -->
		</div>
	
	</div>
	<!--// container -->

	<!-- foot -->
	<%@ include file="/WEB-INF/jsp/ccedu/inc/footer_inc.jsp"%>
