<script>
$(document).scroll(function(){
	var cScroll = document.documentElement.scrollTop;
	
	setTimeout(function(){
		$(".main_pop").each(function(i){
			var scTop = cScroll + 300*i;
			if(i > 0){
				scTop += 10;
			}			
			$(this).css("top",scTop+"px");
		});			
	},100);	
});
$(document).ready(function(){
	pevent();
	/* 상단배너 스크롤 */
	$(".slide_wrap").slick({
		autoplay : true,
		autoplaySpeed : 2500,
		infinite: true,
		speed: 500,
		fade: true,
		arrows: false,
		swipe : false,
		cssEase: 'linear'
	});	
	
	/* 교재 스크롤 */
	$(".main_book_list").slick({
		autoplay : true,
		autoplaySpeed : 2500,
		infinite: true,
		arrows: false,
		slidesToShow: 3,
		swipe : false,		
		slidesToScroll: 1
	});
	
	/* 무료인강 스크롤 */
	$(".main_free_for, .main_free_nav").bind("contextmenu",function(e){ 
		return false;
	});
	$(".main_free_for").slick({
		slidesToShow: 1,
		slidesToScroll: 1,
		arrows: false,
		fade: true,
		swipe : false,
		asNavFor: ".main_free_nav"
	});
	$(".main_free_nav").slick({
		autoplay : false,
		slidesToShow: 3,
		slidesToScroll: 1,
		asNavFor: ".main_free_for",
		arrows: false,
		centerMode: true,
		swipe : false,
		focusOnSelect: true
	});	
	
	/* 이벤트 스크롤 */
	$(".info_event_wrapper").slick({
		autoplay : true,
		autoplaySpeed : 5000,
		infinite: true,
		speed: 500,
		swipe : false,
		arrows: true
	});	

	$(".slide_nav").children().eq(0).addClass("active");
	$('.slide_wrap').on('beforeChange', function(event, slick, currentSlide, nextSlide){
		$(".slide_nav").children().eq(nextSlide-1).removeClass("active");
		$(".slide_nav").children().eq(nextSlide).addClass("active");
	});
	
	$('.main_free_for').on('beforeChange', function(event, slick, currentSlide, nextSlide){
		var videoLength = $(this).children().find("video").length;
		var ele = document.getElementsByClassName("main_free_for");
		
		for(var i=0;i<videoLength;i++){
			ele[0].getElementsByTagName("video")[i].load();
			ele[0].getElementsByTagName("video")[i].pause();			
		}		
	});
});
function slideGo(_this){	
	$(".slide_nav a").removeClass("active");
	$(_this).addClass("active");
	$(".slide_wrap").slick("slickGoTo",$(_this).index());	
}
function setCookie(name, value, expiredays){
	var todayDate = new Date();
		todayDate.setDate (todayDate.getDate() + expiredays);
		document.cookie = name + "=" + escape(value) + "; path=/; expires=" + todayDate.toGMTString() + ";";
}

function closeWin(cnt){
	 if($("#close_view"+cnt).is(":checked")){
		 $.cookie('main_pop'+cnt , 'sunday'+cnt, {expires: new Date(+new Date() + (24 * 60 * 60 * 1000)) });// 쿠키 생성
		 layerPop(cnt);
	 } else {
		 layerPop(cnt);
	 }
}
function layerPop(cnt){
	$('#main_pop'+cnt).css('display' , 'none');
}
function pevent(){	
	for(var i = 0; i<'${fn:length(noticePopList)}';i++){	
		var name = "sunday"+i ;		
		
		getCookie(name);		
		function getCookie(name){			
			var nameOfCookie = name + "=";
			var x = 0;
			while (x <= document.cookie.length){
				var y = (x + nameOfCookie.length);
				if (document.cookie.substring(x, y) == nameOfCookie){
				if ((endOfCookie = document.cookie.indexOf(";", y)) == -1){
				endOfCookie = document.cookie.length;
				}
				return unescape (document.cookie.substring(y, endOfCookie));
				}
				x = document.cookie.indexOf (" ", x) + 1;
				if (x == 0) break;
			}
			return "";
		}
		if(getCookie("main_pop"+i) != "sunday"+i){			
			$('#main_pop'+i).css('display','block');
		}
		
		$("#main_pop"+i).css("left","calc(50% - "+$(".wrapper").width()*0.5+"px - 300px");
		(i > 0) ? $("#main_pop"+i).css("top",300*i+10+"px") : $("#main_pop"+i).css("top","0px");
	}
}
