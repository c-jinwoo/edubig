					if(!fnCheckboxChecked($('#agree10'), "고유식별정보 수집․  이용 동의여부에<br> 동의해주시기 바랍니다.")) return false;
	    		}
	    		
	    		// 응시장소 추가
	    		if(!fnValiCheck($("#cc_place_no"), "응시장소를 선택해주세요.")) return false;
	    		
	    		if(!fnValiCheck($("#mobile2"), "휴대폰 가운데 번호를 입력해주세요.")) return false;
	    		if(!fnValiCheck($("#mobile3"), "휴대폰 뒷 번호를 입력해주세요.")) return false;
	    		
	    		if(!fnValiMinCheck($("#mobile2"), "휴대폰 가운데 번호를 입력해주세요.", 4 )) return false;
	    		if(!fnValiMinCheck($("#mobile3"), "휴대폰 뒷 번호를 입력해주세요.", 4 )) return false;
	    		
	    		if($("#telNo2").val() != '' || $("#telNo3").val() ) {
		    		if(!fnValiMinCheck($("#telNo2"), "유선전화 가운데 번호를 입력해주세요.", 3 )) return false;
		    		if(!fnValiMinCheck($("#telNo3"), "유선전화 뒷 번호를 입력해주세요.", 4 )) return false;
	    		}
		    		
	    		// 핸드폰 번호
	    		if($("#mobile1").val() != '' && $("#mobile2").val() != '' && $("#mobile3").val() != '') {
	    			$("#moblPhon").val($("#mobile1").val() +'-'+ $("#mobile2").val() +'-'+ $("#mobile3").val());
	    		}
	    		
	    		if($("#telNo2").val() != '' && $("#telNo3").val() != '') {
	    			$("#telNo").val($("#telNo1").val() + "-" + $("#telNo2").val() + "-" + $("#telNo3").val());
	    		}
	    		
	    		// 이메일
	    		if(!fnValiCheck($("#email1"), "이메일 아이디를 입력해주세요.")) return false;
	    		if(!fnValiCheck($("#email2"), "이메일 주소를 입력해주세요.")) return false;
	    		
	    		if($("#email1").val() != '' && $("#email2").val() != '') {
