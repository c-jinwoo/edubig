		<c:if test="${examMap.PAY_AT eq 'Y' && REQUEST_DATA.pageMode eq 'insert' }">
			// 결제 정보 설정
			function fnSetUpInfo(){
				var payType = $('input[name=PayM]:checked').val();
					
				if(!$('input[name=PayM]:checked').is(":checked")){
					fnModalMsg("결제 구분을 선택해주세요.");
				}
				
				//PC / MB 구분 
				var UserAgent = navigator.userAgent;
				var LGD_OSTYPE_CHECK = 'P';
				if (UserAgent.match(/iPhone|iPod|Android|Windows CE|BlackBerry|Symbian|Windows Phone|webOS|Opera Mini|Opera Mobi|POLARIS|IEMobile|lgtelecom|nokia|SonyEricsson/i) != null || UserAgent.match(/LG|SAMSUNG|Samsung/) != null)
				{

					LGD_OSTYPE_CHECK = 'M';
				}
				
				$('#LGD_OSTYPE_CHECK').val(LGD_OSTYPE_CHECK);
				
				$('input[name=LGD_CUSTOM_USABLEPAY]').val('SC' + payType.substr(2,4));
				window.open("about:blank","payWinPop","width=650,height=650");
				var frm = document.frm;
				frm.action = "/ccedu/pay/applicationOpenUrl.do";
				frm.target = "payWinPop";
				frm.submit();
			}
	    </c:if>

		/*
		 * 인증결과 처리
		 */
		function payment_return() {
			var fDoc;
			
				fDoc = lgdwin.contentWindow || lgdwin.contentDocument;
			
				
			if (fDoc.document.getElementById('LGD_RESPCODE').value == "0000") {
				//alert(document.getElementById("LGD_PAYKEY").value);
				document.getElementById("LGDPaykey").value = fDoc.document.getElementById('LGD_PAYKEY').value;
				fnSubmit();
			} else {
				alert("LGD_RESPCODE (결과코드) : " + fDoc.document.getElementById('LGD_RESPCODE').value + "\n" + "LGD_RESPMSG (결과메시지): " + fDoc.document.getElementById('LGD_RESPMSG').value);
				closeIframe();
			}
		}
		
		
		/*
		 * submit
		 */
		function fnSubmit(){
			var frm = document.frm;
			
			$.ajax({
				url : "/ccedu/cc/placeChkAjax.do",
				data : $("#frm").serialize(),
				dataType : "JSON",
				type : "get",
				success : function(data){
					if(data.PLACE_CHK == 'Y'){
						frm.target = "";
						frm.action = "/ccedu/cc/applicationRegit.do";			
						frm.submit();
					} else {
						
						var opTag = "<option disabled='disabled' value='" + data.CC_PLACE_NO + "'>" + data.MT_CC_PLACE + " - " + data.PLACE_TITLE + " ( " + data.PLACE_CNT + " / " + data.PLACE_CNT +" ) 정원초과</option>" ;
						$('#cc_place_no option:selected').replaceWith(opTag);
						fnModalMsg("수용 가능 인원이 초과되었습니다. 다른 장소 선택 바랍니다.");
						return false;
					}
					
				},
				error : function(e){
					fnModalMsg("error :" + e.responseText);
				}
			});
			
		}
		
		
	    //파일업로드 AJAX
	    function uploadFileAjax(obj){
	    	
	    	$('#fileNM').val($(obj).val());
	    	
	    	 var form = $('#frm')[0];
	    	 var data = new FormData(form);
	    	
	    	$.ajax({
			    type : "POST", 
			    enctype: 'multipart/form-data',
			    url : "/ccedu/cc/ccLicenseAjax.do",
			    contentType: false,
			    data :  data,
			    processData: false,
			    success : function( data ) {
	    			var result = data.split(",");
	    			$('#savPath').val(result[0]);
	    			$('#orgFile2').val(result[1]);
	    			$('#savFile').val(result[2]);
	    			$("#photoZone").show();
	    			$(".u_photo > img").attr("src",result[0]+'/'+result[2]);
			    }
	    	});
	    }
	    
		
		
		
		 
