	    		if(!fnValiCheck($("#address2"), "상세주소를 입력해주세요.")) return false;
	    		
	    		if(!fnValiCheck($("#mtEdu"), "학력을 선택해주세요.")) return false;
	    		if(!fnValiCheck($("#job"), "직업을 선택해주세요.")) return false;
	    		

				if('insert' == $("#pageMode").val() || 'update' == $("#pageMode").val()) {
	    			
		    		var fileNm = $("#fileNM").val();
		    		if (fileNm != "") {
		    		 
		    		    var ext = fileNm.slice(fileNm.lastIndexOf(".") + 1).toLowerCase();
		    		 
