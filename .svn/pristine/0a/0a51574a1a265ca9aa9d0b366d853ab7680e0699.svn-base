	   function certifiResult() {
		   
		   if(!fnValiMinCheck($("#examNo"), "수료코드를 확인해주세요.", 12)) return false;
		   $.ajax({
			   
			   url : "/ccedu/cc/certificateCheckInfo.do",
			   data : 
				{
					examNo : $("#examNo").val()
				},
				
			   dataType : "json",
			   type : 'post',
			   success : function(data) {
				   	
				   $("#examNo").val('');
				   
				   if(data != "NOTDATA") {
					   
					   $(".etc_center").hide();
					   $(".mgt_first").show();
					
	    			   var html = "";
	    			   html += "<tr>";
	    			   html += "	<th scope='row'>이름</th>";
		    		   html += "	<td>"+data.MBERNM+"</td>";
		    		   html += "</tr>";
		    		   html += "<tr>";
	    			   html += "	<th scope='row'>생년월일</th>";
		    		   html += "	<td>"+data.BRTHDY+"</td>";
		    		   html += "</tr>";
		    		   html += "<tr>";
	    			   html += "	<th scope='row'>과정명</th>";
		    		   html += "	<td>"+data.COURSETITLE+"</td>";
		    		   html += "</tr>";
		    		   html += "<tr>";
	    			   html += "	<th scope='row'>수료일자</th>";
	    			   html += "	<td>"+ data.COMPDATE +"</td>";
		    		   html += "</tr>";
		    		   $("#result").html(html);
					
				   } else {
					   $(".mgt_first").hide();
					   $(".etc_center").show();
				   }
				
				},
				
				error : function(e) {
					fnModalMsg("error :" + e.responseText);
				}
			}); // ajax
		}
		
	</script>
