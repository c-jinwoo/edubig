	 	// 수강신청 버튼 클릭
	 	function insertLecture() {
	 		
	 		  
	 		
	 		
	 		if(!fnValiCheck($("#mtCompDept1"), "단체기관을 선택해주시기 바랍니다.")) {
	 			return false;
	 		}
	 		if($("#mtCompDept1").val() == 'CD0001') {
		 		if(!fnValiCheck($("#mtCompDept2"), "학교를 선택해주시기 바랍니다.")) return false;
	 		}
	 		if(!fnValiCheck($("#compNm"), "단체명을 확인해주시기 바랍니다.")) return false;
	 		if(!fnValiCheck($("#slAppNm"), "담당자명 성명을 확인해주시기 바랍니다.")) return false;
	 		if(!fnValiCheck($("#mobile2"), "휴대폰 가운데번호를 확인해주시기 바랍니다.")) return false;
	 		if(!fnValiCheck($("#mobile3"), "휴대폰 뒷번호를 확인해주시기 바랍니다.")) return false;
	 		
	 		if($("#telNo2").val() != '' || $("#telNo3").val() ) {
	    		if(!fnValiMinCheck($("#telNo2"), "유선전화 가운데 번호를 입력해주세요.", 3 )) return false;
	    		if(!fnValiMinCheck($("#telNo3"), "유선전화 뒷 번호를 입력해주세요.", 4 )) return false;
    		} 
	 		
	 		if(!fnValiCheck($("#email1"), "이메일 아이디를 확인해주시기 바랍니다.")) return false;
	 		if(!fnValiCheck($("#email2"), "이메일 주소를 확인해주시기 바랍니다.")) return false; 
	 		
	 		$("#slAppEmail").val($("#email1").val()+"@"+$("#email2").val());
	 		
	 		if(!fnValiEmail($("#slAppEmail"), "이메일 형식이 올바르지않습니다.")) return false;
			
	 		if(!fnValiCheck($("#eduSdate1"), "교육희망1일시를 입력해주세요.")) return false;
	 		
	 		
	 		$("input[name^=eduSdate]").each(function(i) {
	 			if($("input[name^=eduSdate"+(i+1)+"]").val() != '') {
	 				var choiceDate = $("input[name^=eduSdate"+(i+1)+"]").val();
 	 				if('${toDay}' >= $("#eduSdate"+(i+1)).val()) {
		 				modal({
		 	                type: 'alert',
		 	                title: '알림',
		 	                text: "오늘날짜 이후로 입력해주시기 바랍니다.",
		 	                callback: function(result) {
		 	                    $(':input:text[name^=eduSdate'+(i+1)+']').focus();
		 	                }
		 	            });
		 				return false;
	 				}
	 			}
	 		});
	 	    
	 		if(!compare(1)) return false;
	 		if(!compare(2)) return false;
	 		if(!compare(3)) return false;
	 		
	 	    if(!$('input:radio[name=slLocCd]:checked').val()) {
	 	        modal({
	 	                type: 'alert',
	 	                title: '알림',
	 	                text: "교육장소를 선택해주세요.",
	 	                callback: function(result) {
	 	                    $(':input:radio[name=slLocCd]').focus();
	 	                }
	 	            });
	 	            return false;	
	 	    }
	 	    
	 	    if($('input:radio[id=slLocCd_4]:checked').val()) {
	 	    	if(!fnValiCheck($("#slLocEtc"), "교육장소를 입력해주시기 바랍니다.")) return false;
	 	    }
	 	    
	 	    if(!$('input:radio[name=slTargetCd]:checked').val()) {
	 	        modal({
	 	                type: 'alert',
	 	                title: '알림',
	 	                text: "대상자를 선택해주세요.",
	 	                callback: function(result) {
	 	                    $(':input:radio[name=slTargetCd]').focus();
	 	                }
	 	            });
	 	            return false;	
	 	    }
	 	    
	 	    if(!fnValiCheck($('#slPcnt'),'참석인원을 입력해주세요.')) return false;
	 	    
	 	    if($("#slPcnt").val() == '0') {
	 	    	modal({
	 	    		type: 'alert',
	 	            title: '알림',
	 	            text: "참석인원이 0명이 될 수 없습니다.",
	 	            callback: function(result) {
	 	            	$(':input:text[name=slPcnt]').focus();
	 	            }
	 	        });
	 	    	
	 	    	return false;
	 	    }

	 	    if(!$('input:checkbox[name=mtFcltsCodeArr]:checked').val()) {
	 	    	modal({
	 	    		type: 'alert',
	 	            title: '알림',
	 	            text: "교육 시설정보를 선택해주세요.",
	 	            callback: function(result) {
	 	            	$(':input:radio[name=mtFcltsCodeArr]').focus();
	 	            }
	 	        });
	 	    	
	 	    	return false;	
	 	    }
	 	    
	 	    if(!$('input:radio[name=slVpathCd]:checked').val()) {
	 	        modal({
	 	                type: 'alert',
	 	                title: '알림',
	 	                text: "방문경로를 선택해주세요.",
	 	                callback: function(result) {
	 	                    $(':input:radio[name=slVpathCd]').focus();
	 	                }
	 	            });
	 	           
	 	        return false;	
	 	    }
	 	    
	 	   if($('input:radio[id=slVpathCd_4]:checked').val()) {
	 	    	if(!fnValiCheck($("#slVpathEtc"), "방문경로를 입력해주시기 바랍니다.")) return false;
	 	    }
	 	   
	 	   if(!fnCheckboxChecked($('#agree2'), "개인정보 수집 및 이용에 동의해<br>주시기 바랍니다.")) return false;
	 	    
	 		var eduCount = 0;
	 		
	 		$("input[id^=eduSdate]").each(function(i){
	 			if($(this).val() != '') eduCount++;
	 		});
	 		$('#eduCount').val(eduCount);
			
	 		fnConfirm("등록하시겠습니까?", function(result) {
	 			
	 			if(result) {
		 		
		 			$("#pageMode").val("insert");
			 		$("#slAppEmail").val($("#email1").val()+"@"+$("#email2").val());
		 			$("#slAppTel1").val($("#mobile1").val()+"-"+$("#mobile2").val()+"-"+$("#mobile3").val());
		 			if($("#telNo2").val() != '' && $("#telNo3").val() != '') {
		 				$("#slAppTel2").val($("#telNo1").val()+"-"+$("#telNo2").val()+"-"+$("#telNo3").val());
		 			}
		 			
		 			var ckbs2 = $("input[name='mtFcltsCodeArr']:checked");
		 			ckbs2 = ckbs2.map(function(){
		 				var value = $(this).val();
		 				return value;
		 			}).get().join(",");
		 			
		 			$("#mtFcltsCode").val(ckbs2); 
		 			
		 			
		 		// 교육희망 일시 JSON 형식으로 넘기기
		 			var list = [];
		 			
		 			$('input[name^=eduSdate]').each(function(i){
		 				
		 				var data = {};  
		 				
		 				data.eduSdate = $('input[name=eduSdate'+(i+1)+']').val().replace(/-/g,"").toString()
		 								+ $('select[name=eduSdate_hh'+(i+1)+']').val().toString()
		 								+ $('select[name=eduSdate_mm'+(i+1)+']').val().toString();
		 				
		 				data.eduEdate = $('input[name=eduSdate'+(i+1)+']').val().replace(/-/g,"").toString()
		 				+ $('select[name=eduEdate_hh'+(i+1)+']').val().toString()
		 				+ $('select[name=eduEdate_mm'+(i+1)+']').val().toString();
		 				
		 				list.push(data);
		 				
		 			});
		 			
		 			$('#listParam').val(JSON.stringify(list));
		 			$("#frm").attr("action", "/ccedu/visit/insertLecture.do"); 
		 			$("#frm").submit();
	 			} else {
	 				fnModalMsg("찾아가는 교육신청을 취소하셨습니다.");
					return false;
	 			}
	 		});
	 	}
	 	
	 	
