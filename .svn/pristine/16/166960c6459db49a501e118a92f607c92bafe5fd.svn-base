.content > .openMenu {
  position: absolute;
  top: 50%;
  right:0;
  width: 38px;
  height: 68px;
  margin-top: -34px;
  padding:0;
  background: url(../img/menu.png) no-repeat 100% 100% / 100% 100%;
  border: 0px;
  z-index: 102;
  transition-duration: 0.4s;
  cursor: pointer;
}

.content > .indexMenu {
	position: absolute;
	z-index: 102;
	right:0;
	background:#280138;
	background-size: 100% 100%;
	width: 320px;
	height: 720px;
	transition-duration: 0.4s;
	display: none;
}

.content > .indexMenu > .closeMenu {
  position: absolute;
  z-index: 103;
/*   top: 1%; */
  right: 1px;
  border: 1px;
  background: url(../img/menu_close.png) no-repeat;
  background-size: 100% 100%;
  height: 64px;
  width: 64px;
  cursor: pointer;

}

.content > .indexMenu > .list{
  position: relative;
  top : 10%;
  cursor: pointer;
  font-family: 'Nanum Gothic';
/*   font-family: 'NanumSquere'; */
  font-weight: 700;
}

.content > .indexMenu > .list > .mainTitle {
	color: #FFF;
	font-size: 23px;
	/*   font-size: 1.4em; */
	/* 	margin: 14px 50px 10px 20px; */
	border: 1px;
	text-align: left;
	background-color: hsla(0, 10%, 10%, 0);
	text-indent: 10px;
	cursor: pointer;
	width: 213px;
	height: 33px;
	margin : 12px 0px 16px 0px;
	letter-spacing:-1px;
}

