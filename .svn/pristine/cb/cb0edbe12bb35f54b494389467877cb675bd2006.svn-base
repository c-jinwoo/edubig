@import url(./font.css);
html,body{
	width:100%;
	height:100%;
	margin:0;
	padding:0;
	overflow:hidden;
}
.content {
	/*width: 100%;
	height: 100%;
	overflow: hidden;*/
	position: absolute;
	top: 0;
	left: 0;
	width:1280px;
	height:760px;
}
.content > .controlBar{
	position: absolute;
	width: 100%;
	bottom : 0px;
	height: 40px;
	outline: 1px solid #30353b;
	border-top: 1px solid #7B7B7D;
	background: linear-gradient(to right, #1B2530, #484849);
	padding-top: 0px;
	transition-duration: 0.4s;
	z-index: 200;
}
.content > .controlBar .fl{
	position:relative;
	float:left;
}
.content > .controlBar .fr{
	position:relative;
	float:right;
}
.content > .controlBar > div{
	height:100%;
}
.content > .controlBar > .logo{
	margin-left:10px;
}
.content > .controlBar > .logo img{width:auto;height:100%;}
.content > .controlBar > .timeLine {
	width: 0%;
	height:auto;
	top: 15.5px;
	background: #444444;
	padding-top: 9px;
	z-index: 250;
	display:none;
}
.content > .controlBar > .timeLine > .progress{
	position: absolute;
	background: #35b7e8;
	width: 0%;
	padding-top: 9px;
	top: 0px;
}
.content > .controlBar > .pageCtrl{
	margin-right:10px;
}
.content > .controlBar > .pageCtrl > div{
	margin-right:10px;
}
.content > .controlBar > .pageCtrl > .nFS_btn{
	top:7px;
	width: 30px;
	height: 26px;
	border: 0px;
	color: #ffffff;
}
.content > .controlBar > .pageCtrl > .nFS_btn > button{
	width: 100%;
	height: 100%;
	border: 0px;
	cursor: pointer;
}
.content > .controlBar > .pageCtrl > .digClock{
	height:40px;
	line-height:40px;
}
.content > .controlBar > .pageCtrl > .curPage{
    font-size:24px;	    color:#35B7E8;		width:30px;
}
.content > .controlBar > .pageCtrl > .pageSlash{
    font-size:20px;	    color:#666666;		width:10px;
}
.content > .controlBar > .pageCtrl > .totalPage{
    font-size:24px;	    color:#ffffff;		width:30px;
}
.content > .controlBar > .pageCtrl > .movePrev > button {
  background: url(../img/icon_pre.png) no-repeat;
}
.content > .controlBar > .pageCtrl > .moveNext > button {
  background: url(../img/icon_next.png) no-repeat;
}
.content > .controlBar > .pageCtrl > .movePrev > button:hover,
.content > .controlBar > .pageCtrl > .moveNext > button:hover{
    background-position: -30px 0px;
}
.content > .controlBar > .pageCtrl > .movePrev > button.toggle,
.content > .controlBar > .pageCtrl > .moveNext > button.toggle{
    background-position: -0px -30px;
}
.content > .controlBar > .pageCtrl > .movePrev > button.toggle:hover,
.content > .controlBar > .pageCtrl > .moveNext > button.toggle:hover{
    background-position: -30px -30px;
}

.content > .controlBar > .playerCtrl{
	margin-left:20px;
	margin-right:20px;
	display:none;
}
.content > .controlBar > .playerCtrl > .ctrl_btn{
	top:7px;
	width: 26px;
	height: 26px;
	border: 0px;
	color: #ffffff;
	margin-left:10px;
}
.content > .controlBar > .playerCtrl > .ctrl_btn > button{
	border: 0px;
	width: 100%;
	height: 100%;
	cursor: pointer;
}
.content > .controlBar > .playerCtrl > .clock{
	color:#ffffff;
	font-size:12px;
	margin-left:15px;
	line-height:40px;
	width:30px;
}
.content > .controlBar > .playerCtrl > .clock.curTime{
	margin-left:0;
}
.content > .controlBar > .playerCtrl > .clock.slash{
	width:1px;
}
.content > .controlBar > .playerCtrl > .soundLine{
	top: 15.5px;
	width: 45px;
	height:auto;
	background: #444444;
	padding-top : 9px;
	margin-left:5px;
}
.content > .controlBar > .playerCtrl > .soundLine > .soundProgress {
	position: absolute;
	width: 50%;
	padding-top: 9px;
	top: -0px;
	background: #35b7e8;
}
.content > .controlBar > .playerCtrl > .play > button {
  background: url(../img/icon_play.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .pause > button {
  background: url(../img/icon_pause.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .replay > button {
  background: url(../img/icon_replay.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .trackBtn > button {
  background: url(../img/icon_text.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .playbackRW > button {
  background: url(../img/icon_playbackRW.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .playbackRate {
	color:#ffffff;
	font-size:12px;
	margin-left:15px;
	width:16px;
	line-height:40px;
}
.content > .controlBar > .playerCtrl > .playbackFF > button {
  background: url(../img/icon_playbackFF.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .playbackRate,
.content > .controlBar > .playerCtrl > .playbackFF{
	margin-left:0;
}
.content > .controlBar > .playerCtrl > .sound > button {
  background: url(../img/icon_vol.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .fullScreen > button {
  background: url(../img/icon_scale.png) no-repeat;
}
.content > .controlBar > .playerCtrl > .ctrl_btn > button:hover{
    background-position: -26px 0px;
}
.content > .controlBar > .playerCtrl > .ctrl_btn > button.toggle{
    background-position: -0px -26px;
}
.content > .controlBar > .playerCtrl > .ctrl_btn > button.toggle:hover{
    background-position: -26px -26px;
}
.content > .controlBar > .nextAlert {
	position: absolute;
	width: 145px;
	height: 32px;
	right: 0;
	bottom: 40px;
	transition-duration: 1.0s;
	background: url(../img/moveAlert.png) no-repeat;
	display: none;
}
.content > .controlBar div .tooltips {
	-webkit-transition: bottom .3s ease-in-out, opacity .3s ease-in-out;
	-moz-transition: bottom .3s ease-in-out, opacity .3s ease-in-out;
	transition: bottom .3s ease-in-out, opacity .3s ease-in-out;

  -webkit-box-shadow: 0px 0px 3px 1px rgba(50, 50, 50, 0.4);
	-moz-box-shadow: 0px 0px 3px 1px rgba(50, 50, 50, 0.4);
	box-shadow: 0px 0px 3px 1px rgba(50, 50, 50, 0.4);

  -webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;

	color: #ffffff;
	background-color: #35B7E8;
	font-size: 12px;
	margin-bottom: 10px;
	padding: 7px 5px;
	position: absolute;
	width: auto;
	min-width: 60px;
	max-width: 300px;
	word-wrap: break-word;
	z-index: 9999;
	opacity: 0;
	left: -9999px;
	top: -9999px;
	width: 55px;
    margin-left: 7px;
	text-align: center;
}
.content > .controlBar div .tooltips.hover{
	position: relative;
	top: -70px;
	left: -30px;
	opacity: 1;
}
.content > .controlBar div .tooltips::after{    
    content: "";
    position: absolute;
    top: 100%;
    left: 50%;
    margin-left: -5px;
    border-width: 5px;
    border-style: solid;
    border-color: #35B7E8 transparent transparent transparent;
}
