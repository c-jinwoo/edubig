	<script type="text/javascript">
		$(document).ready(function(){
			onlyNum2('ssn1');
		});
	
		function fnSelectType(type){
			$('input[name=mberType]').val(type);
			$('#frm').submit();
		}
		
		// 공인인증서 팝업
		function fnCertFormSubmit(){
			var certForm = document.certForm;
	
			$('#ssn').val($('#ssn1').val()+$('#ssn2').val());
			window.open("", "auth_popup", "width=432,height=560,scrollbar=yes");
	
			certForm.target = "auth_popup";
			certForm.submit();
		}
		
		//인증 유형 설정
		function fnCertType(type){
			
			var mT = $('input[name=mberType]').val();
			$('#confirmGubun').val(type);
			
			ajaxSubmit('/ccedu/user/memberDupChkAjax.do',
			        {  confirmGubun : $('#confirmGubun').val()
						, di : $('input[name=di]').val()
			        }, 'json',
			        eval('resultDupChk')
			);
			
		}
		
		function fnGoSubmit(){
			if(!fnValiCheck($('#confirmGubun'),'실명인증을 해주세요.')) return false;
			if(!fnValiCheck($('input[name=di]'),'실명인증을 해주세요.')) return false;

			$('#frm').submit();
		}
		
	
		function resultDupChk(data){
			
			var mT = $('input[name=mberType]').val();
			
			if(mT == 'N'){
			    if(data > 0) {
			    	fnConfirm("가입정보가 있습니다.<br> 로그인 페이지로 이동하시겠습니까?", function(result) {
		    			if(result){
			    			$("#frm").attr("action", '/ccedu/common/login.do');
			    			$("#frm").submit();
		    			}
		    			return false;
					});
		    		return false;
			    }else if(data == 0) {
			    	fnGoSubmit();
			    }
			}else{
				fnGoSubmit();
			}
			
		}
	
		
		//레이어 초기화
		function fnLayerClean(){
			$('#mberNm').val('');
			$('#ssn').val('');
			$('#ssn1').val('');
			$('#ssn2').val('');
		}
