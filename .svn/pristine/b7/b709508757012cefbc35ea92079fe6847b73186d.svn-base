	.foot_info {padding:30px; font-size:18px; line-height:30px;}
	.foot_info .copyright {position:relative; margin-top:20px; opacity:0.5;}

	/*2018-08-28 김동인 퍼블리싱 작업*/
	.footlinks_fmlst .footlinks li.point a {color:#c6664f}
}

