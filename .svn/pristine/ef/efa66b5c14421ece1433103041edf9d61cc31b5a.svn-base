.footlinks_fmlst .fmlst .site_list li a:hover span,
.footlinks_fmlst .fmlst .site_list li:hover a{color:#fff; font-family:ngb;background:#f13748;}
.foot_info {position:relative; width:100%; background:#e3e3e3; color:rgba(255,255,255,0.7); font-size:12px; line-height:22px; z-index:50;}
.foot_info .copyright {position:absolute; right:0; bottom:0;}
.foot_info .foot_inner{width:100%; padding:23px 0; overflow:hidden;}
.foot_info .foot_logo{position:relative; display:inline-block; width:146px; height:57px; float:left; margin-top:5px;}
.foot_info .txt_info{position:relative; display:inline-block; color:#666; margin-left:25px;}
.footlinks_fmlst .footlinks li.point > a:before {content:none;}
.footlinks_fmlst .footlinks li:nth-child(2) > a:before {content:none;}
.footerWebAccessibility { float: right; margin-top: -88px; }

