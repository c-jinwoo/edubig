@media (max-width:1080px){
	#content {min-height:calc(100vh - 288px); padding-left:0;}
	.wrapper,
	.cm_wrapper{width:768px;}
	
	.open_allmenu {overflow:hidden;}
	.open_allmenu #wrap {position:fixed; width:100%; height:100%; overflow:hidden;}
	.all_menu_wrap {position:absolute; top:80px; left:0; width:85%; height:calc(100vh - 80px); background:#575e89; transform:translateX(-100%); -webkit-transform:translateX(-100%); transition:.3s; -webkit-transition:.3s; z-index:100;}

	.open_allmenu .all_menu_wrap {transform:translateX(0); -webkit-transform:translateX(0); transition:.3s; -webkit-transition:.3s;}
	.all_menu {width:280px; height:calc(100vh - 80px); padding-top:40px; background:#4e5583;}
	.all_menu > li > a {display:block; position:relative; padding-left:25px; color:#c5c6d3; font-size:30px; line-height:80px; font-weight:bold; z-index:20;}
	.all_menu > li.active > a {color:#fff;}
	.all_menu > li.active > a:before {content:''; position:absolute; top:50%; right:50px; width:24px; height:24px; margin-top:-10px; background:url('../images/m_ico_gnb_next.png') no-repeat center center;}
	.all_menu > li > .in_depth {position:absolute; top:0; left:0; width:1px; height:1px; padding:0; z-index:15; overflow:hidden;}
	.all_menu > li.active .in_depth {width:100%; height:100%; padding-left:280px; overflow-y:auto; -webkit-overflow-scrolling: touch;}
	.all_menu > li > .in_depth .in_depth_tit_p {position:relative; padding:40px 0 22px 30px; color:#fff; font-size:40px; line-height:82px;}
	.all_menu > li > .in_depth .in_depth_tit_p:before {content:''; position:absolute; bottom:0; left:30px; width:66px; height:4px; background:rgba(255,255,255,0.7);}
	.all_menu > li > .in_depth .in_depth_menu {margin-top:11px; padding-bottom:175px;}
	.all_menu > li > .in_depth .in_depth_menu > li > a {display:block; position:relative; padding-left:30px; color:#c5c6d3; font-size:28px; line-height:80px; font-weight:bold;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.active {color:#fff;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.inul:before {content:''; position:absolute; top:50%; right:30px; width:20px; height:4px; margin-top:-2px; background:#c5c6d3;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.inul:after {content:''; position:absolute; top:50%; right:38px; width:4px; height:20px; margin-top:-10px; background:#c5c6d3;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.inul.active:before {background:#fff;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.inul.active:after {display:none;}
	.all_menu > li > .in_depth .in_depth_menu > li > ul {height:1px; overflow:hidden;}
	.all_menu > li > .in_depth .in_depth_menu > li > a.active + ul {height:auto;}
	.all_menu > li > .in_depth .in_depth_menu > li > ul > li > a {display:block; padding-left:60px; color:#c5c6d3; font-size:26px; line-height:66px; font-weight:bold;}
	
	.gnb_items > ul > li > a {padding: 0px 15px;}
	
	/* 헤더 */
	.quick_menu_renew,
	.slide_nav_wrapper{display:none;}
	.gnb_section .gnb_items > ul > li > a {padding: 0 20px;}
	
	/* 상단 : 슬라이더 */
	.slick-slide img {width:100%;}
	
	/* 중간1 : 강사 */
	.main_star_list .cont_box span {font-size:12px;}
	.main_star_list .cont_box strong {margin-left:5px; font-size:17px;}
	
	/* 서브페이지 : 과정소개 */
	.cont_apply:before{height:82px;}
	
	/* 하단 : 게시판요약 */
	.info_event_tit {font-size:15px;}
	.info_call ul li strong {display:block;}
	
	/* 푸터 */
	.footlinks_fmlst .footlinks li a {font-size:12px;}
	.footlinks_fmlst .footlinks li:nth-child(4){letter-spacing:-2.2px;}
}
