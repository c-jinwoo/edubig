.gnb_section {position:relative; width:100%; height:50px; border-top:1px solid #cdcdcd; border-bottom:1px solid #cdcdcd; background:#FFF;z-index:10;top:0;}
.gnb_section .gnb_outer {height:50px; position:relative;}
.gnb_section .gnb_box {width:1080px; margin: 0 auto;}
.gnb_section .gnb_items {position:relative; width:100%; font-size:0;}
.gnb_section .gnb_items > ul > li {display:inline-block; vertical-align:top; width:calc(100% / 6); text-align:center;}
.gnb_section .gnb_items > ul > li > a {height:50px; padding:0 36px; display:block; font-size:16px; line-height:51px; font-weight:600; color:#666;}
.gnb_section .gnb_s_layer{display:none; width:100%; height:auto; position:absolute; left:0; top:49px; background-color:#eaeaf0; z-index:1200;}
.gnb_section .gnb_s_layer ul{text-align:left; font-size:0;}
.gnb_section .gnb_s_layer li{display:inline-block; vertical-align:middle;}
.gnb_section .gnb_s_layer a{display:block; padding:0 24px; font-size:16px; line-height:65px; color:#666;}
.gnb_section .gnb_s_layer a:hover{color:#e71a3f;}
.gnb_section .gnb_s_layer .mycla{text-align: right;}
.gnb_section .gnb_course_layer.on{display:block;}
.gnb_section .gnb_myclass_layer.on{display:block;}

#gnb > li.active > a,
#gnb > li > a:hover,
#gnb > li > a:focus,
#gnb > li > a:active{color:#e71a3f;}
#gnb > li > .in_depth {position:absolute; left:0; width:1px; height:1px; padding:0; border:1px solid #ccc; background:#f9f9f9; text-indent:-9999px; overflow:hidden;}
#gnb > li.active > .in_depth {width:100%; height:auto; padding:0; text-indent:0;}
#gnb > li > .in_depth .in_depth_menu > li > a {display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center;  float:left; width:20%; height:49px; padding:0 0 0 30px; color:#4e5583; font-size:15px; line-height:19px; font-weight:bold; word-break:keep-all; background-color:#e5e5e5}
#gnb > li > .in_depth .in_depth_menu > li {border-top:1px solid #d5d5d5; overflow:hidden;}
#gnb > li > .in_depth .in_depth_tit_p {position:absolute; top:0; width:330px; height:100%; padding:0 30px;}
#gnb > li > .in_depth .in_depth_tit_p .tit {position:absolute; top:50%; margin-top:-61px; padding-bottom:15px; font-size:22px; line-height:32px;}
#gnb > li > .in_depth .in_depth_tit_p .tit_p {position:absolute; top:50%; margin-top:0; font-size:13px; line-height:23px; opacity:0.7;}
#gnb > li > .in_depth .in_depth_menu {padding-left:330px; letter-spacing:-1px;}
#gnb > li > .in_depth .in_depth_menu > li:first-child {border-top:none;}
#gnb > li > .in_depth .in_depth_menu > li > ul {float:left; width:80%;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li {float:left; width:25%;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a {display:-webkit-flex; display:flex; -webkit-align-items:center; align-items:center; position:relative; width:100%; height:49px; padding:0 0 0 35%; white-space:nowrap; word-break:keep-all;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:hover,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:focus,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:active {color:#4458b7; font-weight:bold;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a > span {display:inline-block; font-size:13px; line-height:21px;}
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:hover span,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:focus span,
#gnb > li > .in_depth .in_depth_menu > li > ul > li > a:active span {border-bottom:2px solid #4458b7;}

.quick_menu_renew{position:absolute; top:137px; right:0; width:118px; z-index:9999;}
.quick_menu_renew .quick_menu_tit{display:block;height:36px;font-size:0;text-indent: -9999;background:url('../images/quick_menu.png') no-repeat center center;margin: 0 auto;}
.quick_menu_renew ul{margin-bottom:13px;}
.quick_menu_renew ul li{width:100%;background:#fff;}
.quick_menu_renew ul li a{display:block;padding:12px 0;border:1px solid #d7d7d7;border-top:0;font-size:12px;text-align:center;color:#797979;}
.quick_menu_renew ul li:first-child a{color:#cc0000;}
.quick_menu_renew > a{font-size:0;text-indent: -9999;display:block;height:23px;background:url('../images/quick_top.png') no-repeat center center;}
.quick_menu_renew > a.top{height:22px;/* background-position:-350px -326px; */}
.quick_menu_renew .btn_quick a{position:absolute; top:276px; left:-23px; width:22px; height:69px; font-size:0; line-height:0; z-index:10;}
.quick_menu_renew .btn_quick .open{display:none}
.quick_menu_renew .btn_quick .close{background-position:-39px -33px}
.quick_menu_renew .btn_quick .open{display:block}
.quick_menu_renew .btn_quick .close{display:none; background-position:-39px -33px}
.quick_menu_renew .btn_quick a{top:65px; left:-42px; width:42px; height:150px; background:#f4f4f4; border:1px solid #cdcdcd; border-right:0; box-sizing: border-box; padding-top:60px; text-align:center;}
.quick_menu_renew .btn_quick a span{display:inline-block; width:14px; font-size:14px; color:#222; line-height:16px;}
.quick_menu_renew .btn_quick a:before{content:''; display:block; width:12px; height:12px; position:absolute; top:25px; right:11px; border:1px solid #222; border-width:0 0 1px 1px;transform:rotate(45deg);-webkit-transform:rotate(45deg);-ms-transition: all 0.3s ease-out;-webkit-transition: all 0.3s ease-out;transition: all 0.3s ease-out;}
.quick_menu_renew .btn_quick a.close:before{border-width:1px 1px 0 0; right:18px}
.quick_menu_renew .btn_quick.open a:before{-webkit-transform:rotate(-135deg); transform:rotate(-135deg); right:18px}

