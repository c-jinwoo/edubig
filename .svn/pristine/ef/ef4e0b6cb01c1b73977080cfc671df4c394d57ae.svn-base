<h2>온라인서점</h2>
	<ul class="lnb_menu">
		<c:forEach items="${courseList}" var="result" varStatus="stat">
			<c:choose>
				<c:when test="${param.mtSubCode eq result.MT_SUB_CODE || param.mtScCode eq result.MT_SUB_CODE}">
					<li class="on">				
				</c:when>
				<c:otherwise>
					<li>				
