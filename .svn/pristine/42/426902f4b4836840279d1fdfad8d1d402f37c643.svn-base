	<title>수료증 진위여부 확인 | 기타</title>
	<%@ include file="/WEB-INF/jsp/ccedu/inc/head_inc.jsp"%>
	<script type="text/javascript">
		
		$(document).ready(function() {
			
			$('#examNo').keyup(function () { 
			    this.value = this.value.replace(/[^0-9-]|^[-]/gi,"");
			});
			
		});
	
		  function autoHypen(pthis) {
	
		      var html = '';
		      var examNo = $(pthis).val();
	
		      if(examNo.length == 4){
		        html += examNo.substr(0, 4);
		        html += '-';
		        $(pthis).val(html);
		      } else if(examNo.length == 7){
		  		html += examNo.substr(0, 4);
				html += '-';
				html += examNo.substr(5, 2);
				html += '-';
				$(pthis).val(html);
		      }
		  }
		
		
		function certifiResult() {
			
			if(!fnValiMinCheck($("#examNo"), "수료코드를 확인해주세요.", 14)) return false;
			$("#pageMode").val('check');
			$("#frm").submit();
		}
		
	</script>
</head>

