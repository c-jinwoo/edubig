.content > .quiz > .board {
	background: url(../img/quiz/background_quiz.png) no-repeat 100% 100% / 100% 100%;
	position: absolute;
	width: 100%;
    height: 100%;
    z-index: 99;
  	
  	
/*     width: 100%; */
}
.content > .quiz > .board > .top {
    color: #333333;
}
.content > .quiz > .board > .top > .symbol{
	position: absolute;
	left: 10%;
	top: 15%;
	font-size: 40px;

}

.content > .quiz > .board > .top > .explain{
  position: absolute;
  left: 30%;
  top: 16%;
  font-size: 30px;
}
.content > .quiz > .board > .top > .explain_1{
    position: absolute;
    left: 45%;
    top: 15%;
    font-family: NanumSquare;
    font-weight: 900;
    letter-spacing: -3px;
    font-size: 34px; 
}

.content > .quiz > .board > .top > .explain_2{
    position: absolute;
	left: 34%;
    top: 25%;
    font-family: NanumSquare;
    font-weight: 900;
    letter-spacing: -4px;
    font-size: 55px;
}

.content > .quiz > .board > .middle{
    position: absolute;
    top: 48.5%;
    left: 30%;
    width: 50%;
    height: 50%;

}
.content > .quiz > .board > .middle > .line{
  border: dashed;
  border-color: #aaaaaa;
  border-width: 1px 3px 3px 1px;
  height: 50%;
  float: left;
  top: 10%;
  position: relative;
}

.content > .quiz > .board > .middle > .result{
  position: relative;
  float: left;
  width: 20%;
  height: 30%;
  margin: 22px;
}
.content > .quiz > .board > .middle > .result > .symbol {
  font-size: 50px;
  text-align: center;
  font-family: sans-serif;
  color: #666666;
  bottom: 15%;
  position: relative;
}
.content > .quiz > .board > .middle > .result > .checkSymbol {
  width : 100%;
  height: 100%;
}
.content > .quiz > .board > .middle > .result > .oSymbol {
  background: url(../img/quiz/oSymbol.png) no-repeat;
  background-size: 100% 100%;
}
.content > .quiz > .board > .middle > .result > .xSymbol {
  background: url(../img/quiz/xSymbol.png) no-repeat;
  background-size: 100% 100%;
}

.content > .quiz > .board > .retryBtn  {
  background: url(../img/quiz/retry.png) no-repeat;
  background-size: 100% 100%;
  width: 120px;
  height: 35px;
  border: 0px;
  top: 80%;
  left: 50%;
  margin-left: -60px;
  position: absolute;
  cursor: pointer;
}

.content > .quiz > .board > .retryBtn:hover  {
  background: url(../img/quiz/retry_over.png) no-repeat;
  background-size: 100% 100%;
}

