    letter-spacing: -1px;
	max-width:90%;
}
.content > .quiz > .page > .question > .symbol {
    font-family: NanumBarunGothic;
    position: relative;
    bottom: -22px;
    font-size: 65px;
    font-weight: 900;
    letter-spacing: -4px;
    color: #000;
}

.content > .quiz > .page > .question > .symbol > .oSymbol {
  position: absolute;
  background: url(../img/quiz/oSymbol.png) no-repeat;
