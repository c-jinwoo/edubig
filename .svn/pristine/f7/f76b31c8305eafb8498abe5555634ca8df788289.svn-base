_this = this;
_this.$browser = getBrowserType();						// 사용 browser
_this.$bolFullScreen = false;							// 전체화면 여부 ( false : 창모드 true : 전체화면 )
_this.$bolPlay = false;									// 재생 여부 ( fasle : 일시정지 true : 재생중 )
_this.$curChaSiNumber	= getURL(1) * 1;				// 콘텐츠 현재 차시 번호
_this.$curPageNumber	= getURL(0) * 1;				// 콘텐츠 현재 페이지 번호
_this.$movieName		= itostr(_this.$curPageNumber) + ".mp4"; 
_this.$moviePath		= "https://cdn.edubig.co.kr/_definst_/mp4:3d1/"+itostr(_this.$curChaSiNumber)+"/"+_this.$movieName+"/playlist.m3u8";

if(location.protocol == "http:"){
	_this.$moviePath = "http://cslabs-cvod.myskcdn.net/_definst_/mp4:bigdata/"+itostr(_this.$curChaSiNumber)+"/"+_this.$movieName+"/playlist.m3u8";	
}
else if(location.protocol == "https:"){
	_this.$moviePath = "https://cdn.edubig.co.kr/_definst_/mp4:bigdata/"+itostr(_this.$curChaSiNumber)+"/"+_this.$movieName+"/playlist.m3u8";		
}

